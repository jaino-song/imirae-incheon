{
    "version": 2,
    "builds": [
        {
            "src": "server/package.json",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/server.js"
        }
    ]
} 